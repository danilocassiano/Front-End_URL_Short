{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector",
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/lib/**/*.js",
        "<node_internals>/**",
        "<node_internals>/**/*.js",
        "${workspaceFolder}/node_modules/**/*.ts",
        "${workspaceFolder}/node_modules/**/*",
        "${workspaceFolder}/lib/**/*.ts",
        "<node_internals>/**/*.ts"
      ],
    }
  ]
}